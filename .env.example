# Mycelium Development Environment Configuration
# Copy this file to .env and customize for your environment

# Telemetry Configuration (opt-in, disabled by default)
# Set to 'true' to enable telemetry collection
TELEMETRY_ENABLED=false
TELEMETRY_ENDPOINT=http://localhost:8080/telemetry
TELEMETRY_TIMEOUT=5
TELEMETRY_BATCH_SIZE=100
TELEMETRY_RETRY_ATTEMPTS=3

# Database Configuration (for M01 agent registry)
# Update with your PostgreSQL credentials
DATABASE_URL=postgresql://mycelium:your_password@localhost:5432/mycelium_registry

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
